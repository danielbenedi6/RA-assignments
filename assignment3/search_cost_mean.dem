#GNU PLOT
set title "Mean of SkipList total search cost with p = 0.5"
set xlabel "N"
set ylabel "Comparisons"
q = 0.5
p = 1 - q
Q = 1/q
L = log(Q)
gamma = 0.5772156649
cost(x) = Q*x*log(x)/log(Q) + x * (Q/L*(gamma-1) + 1/L - Q/2)
plot "search_cost_median_mean.dat" title "Experimental", cost(x) title "Theoretical"
pause -1
